    "import read\n",
    "import dateutil\n",
    "\n",
    "df = read.load_data()\n",
    "#print (df.info())\n",
    "df = df.dropna()\n",
    "#print (df.info())\n",
    "\n",
    "def parse(stp):\n",
    "    a = dateutil.parser.parse(stp)\n",
    "    return a.hour\n",
    "\n",
    "df['hours'] = df['submission_time'].apply(parse)\n",
    "hour = df['hours'].value_counts()\n",
    "\n",
    "print(hour)\n"
   ]
  }
